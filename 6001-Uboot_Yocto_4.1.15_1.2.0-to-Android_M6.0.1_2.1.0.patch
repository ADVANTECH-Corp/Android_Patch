+		clrbits_le32(&imx_ccm->CCGR0, mask);
+#elif defined(CONFIG_MX6UL)
 	u32 mask = MXC_CCM_CCGR3_ENET_CLK_ENABLE_MASK;
 	/* Set AHB clk, since enet clock is sourced from AHB and IPG */
